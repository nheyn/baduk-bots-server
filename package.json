{
  "name": "baduk-bots-server",
  "version": "0.0.1",
  "description": "A react-swarm-ui version of Go(Baduk), that is played using Baduk Zooid Bots",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "clean": "rm -rf node_modules ./dist",
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "node ./dist/index.js",
    "test": "flow check",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nheyn/baduk-bots-server.git"
  },
  "author": "Nick Heyn <nick@heyn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nheyn/baduk-bots-server/issues"
  },
  "homepage": "https://github.com/nheyn/baduk-bots-server#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "flow-bin": "^0.83.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "react": "^16.6.0",
    "react-swarm-ui": "*",
    "ws": "^6.1.0"
  }
}
